  "scripts": {
    "clean": "rm -rf .next",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "npm run lint:fix && npm run test-silent",
    "lint": "npm run lint:next && npm run lint:css && npm run test",
    "lint:fix": "npm run lint:next:fix && npm run lint:css:fix",
    "lint:next": "next lint",
    "lint:next:fix": "next lint --fix",
    "lint:css": "stylelint **/*.{css,scss,sass}",
    "lint:css:fix": "stylelint --fix **/*.{css,scss,sass}",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test-silent": "jest --silent --passWithNoTests",
    "codegen": "npx graphql-codegen"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run fix && npm run build"
